#include <stdio.h>

int main() {
    int a = 10, b = 5;
    int result_and, result_or;
    int condition = 1;

    // Logical AND
    result_and = (a > b) && (b != 0);
    printf("(a > b) && (b != 0) is %d\n", result_and);

    // Logical OR
    result_or = (a == b) || (condition == 1);
    printf("(a == b) || (condition == 1) is %d\n", result_or);

    // Logical NOT
    printf("!condition is %d\n", !condition);

    return 0;
}
