#include <stdio.h>

int main() {
    int a = 10, b = 5;

    // Relational operations
    printf("%d > %d is %s\n", a, b, a > b ? "true" : "false");
    printf("%d < %d is %s\n", a, b, a < b ? "true" : "false");
    printf("%d >= %d is %s\n", a, b, a >= b ? "true" : "false");
    printf("%d <= %d is %s\n", a, b, a <= b ? "true" : "false");
    printf("%d == %d is %s\n", a, b, a == b ? "true" : "false");
    printf("%d != %d is %s\n", a, b, a != b ? "true" : "false");

    return 0;
}
